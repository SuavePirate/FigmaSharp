<?xml version="1.0"?>
<package >
  <metadata>
    <id>FigmaSharp.Cocoa</id>
    <title>FigmaSharp.Cocoa</title>
    <version>$(FSMajorVersion).$(FSMinorVersion).$(FSCoreMacVersion)-alpha</version>
    <authors>Jose Medrano</authors>
    <licenseUrl>https://www.microsoft.com/es-es/</licenseUrl>
    <projectUrl>https://www.microsoft.com/es-es/</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/netonjm/FigmaSharp/master/icons/image-logo.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Say hello to #FigmaSharp and enhance your .NET applications taking advantage of Figma's component system creating beautiful views in your application!</description>
	 <releaseNotes>https://www.microsoft.com/es-es/</releaseNotes>
    <copyright>Jose Medrano.</copyright>
    <tags>figma designer design</tags>
    <dependencies>
      <dependency id="FigmaSharp" version="$(FSMajorVersion).$(FSMinorVersion).$(FSCoreMacVersion)-alpha" />
      <dependency id="FigmaSharp.Views" version="$(FSMajorVersion).$(FSMinorVersion).$(FSViewVersion)-alpha" />
      <dependency id="FigmaSharp.Views.Cocoa" version="$(FSMajorVersion).$(FSMinorVersion).$(FSViewMacVersion)-alpha" />
      <dependency id="Newtonsoft.Json" version="$(NewtonsoftNugetVersion)" />
      <dependency id="System.Drawing.Common" version="$(SystemDrawingCommonVersion)" /> 
    </dependencies>
  </metadata>

  <files>
    <file src="..\FigmaSharp\FigmaSharp.Cocoa\bin\Release\*.dll" target="lib/net472" />
  </files>

</package>